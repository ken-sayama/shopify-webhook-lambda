{
    "extends": ["airbnb-base", "eslint:recommended", "plugin:import/errors"],
    "plugins": [
         "promise"
     ],
    "env": {
      "node": true,
      "mocha": true,
      "es6": true
    },
    "settings": {
      "import/core-modules": ["aws-sdk"]
    },
    "globals": {},
    "rules": {
      "comma-dangle": 0,
      "padded-blocks": 0,
      "no-console": 0,
      "no-unused-vars": [1, {"args": "none"}],
      "no-var": 2,
      "one-var": [2, "never"],
      "indent": [2, 4, {"SwitchCase": 1}],
      "eqeqeq": 2,
      "arrow-body-style": 2,
      "strict": 0,
      "import/no-extraneous-dependencies" : 0,
      "import/no-unresolved": [2, { "commonjs": true, "amd": true, "ignore": [ "config$" ] }],
      "promise/param-names": 2,
      "promise/always-return": 0,
      "promise/catch-or-return": 1,
      "promise/no-native": 0,
      "prefer-destructuring": 1,
      "prefer-rest-params": 1,
      "prefer-template": 1,
      "prefer-const" : 1,
      "prefer-spread" : 1,
      "prefer-numeric-literals": 1,
      "import/prefer-default-export": 0
    }
  }
  